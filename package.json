{
  "name": "pev-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint src --ext ts",
    "start": "npm-run-all -p start:*",
    "start:server": "node dist/shared/infra/http/server.js",
    "docker": "docker-compose up -d",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:sync": "typeorm-ts-node-commonjs schema:sync -d src/shared/infra/typeorm",
    "typeorm:drop": "npm run typeorm schema:drop",
    "typeorm:reset": "npm run typeorm:drop && npm run typeorm:sync",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/shared/infra/typeorm"
  },
  "dependencies": {
    "@bull-board/api": "^5.0.0",
    "@bull-board/express": "^5.0.0",
    "@sentry/node": "^6.2.5",
    "@sentry/tracing": "^6.4.1",
    "@types/node-cron": "^3.0.7",
    "app-store-server-api": "^0.9.0",
    "aws-sdk": "^2.1472.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^14.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "cognito-express": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.4",
    "date-fns": "^2.16.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "file-type": "16",
    "firebase-admin": "^11.9.0",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "ioredis": "^4.27.3",
    "jwk-to-pem": "^2.0.5",
    "mime": "^2.5.2",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.17",
    "npm-run-all": "^4.1.5",
    "openai": "^4.24.1",
    "pg": "^8.5.1",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^3.1.1",
    "redis-url": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.12",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/mercadopago": "^1.5.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.11.8",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/socket.io": "^2.1.13",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-metadata": "^4.0.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.1.2",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.0.0-pre.63",
